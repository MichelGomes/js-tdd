{
  "name": "js-tdd",
  "version": "1.0.0",
  "description": "Reviewing TDD concepts over JS",
  "main": "index.js",
  "dependencies": {
    "user": "^0.0.0"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.6"
  },  
  "scripts": {
    "lint":"node_modules\\.bin\\eslint src/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "git diff HEAD --quiet && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichelGomes/js-tdd.git"
  },
  "keywords": [
    "jss",
    "tdd",
    "review"
  ],
  "author": "Michel Gomes <gomes.michel@gmail.com>",
  "license": "MIT",
